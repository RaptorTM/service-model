pipeline {
    agent any

    stages {
        stage('Run main.groovy') {
            steps {
                // Запускаем файл main.groovy
                script {
                    def groovyScript = load 'src\\Main.groovy'
                    groovyScript.run_test_script()

                }
            }
        }
    }

    post {
        success {
            echo 'Pipeline succeeded!'
        }
        failure {
            echo 'Pipeline failed!'
        }
    }
}